<scenario>
  <name>TooYoung.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Driver</type>
      <fieldData>
        <field-data>
          <name>age</name>
          <value>15</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>driver</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>TooYoung</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>reason</fieldName>
          <expected>Too Young</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>Rejection</name>
      <anonymous>true</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>org.acme.insurance.exclusion</packageName>
  <imports>
    <imports>
      <org.kie.soup.project.datamodel.imports.Import>
        <type>org.acme.insurance.Driver</type>
      </org.kie.soup.project.datamodel.imports.Import>
      <org.kie.soup.project.datamodel.imports.Import>
        <type>org.acme.insurance.Rejection</type>
      </org.kie.soup.project.datamodel.imports.Import>
    </imports>
  </imports>
  <ksessions>
    <string>defaultKieSession</string>
  </ksessions>
</scenario>